}
#body .games .content span a {
	font-size: 14px;
    text-align: center;;
	line-height: 40px;
}
#body div div .section div div span a:hover {
	color: #fff;
}
#footer {
	background: url(/Images/bg-footer2.png) repeat-x bottom left;
	margin: 0;
	padding: 0;
}
#footer > div {
	background: url(/Images/bg-footer.png) no-repeat bottom center;
	margin: 0;
	text-align: center;
	padding: 6% 0 0;
	display: block;
}
#footer #brand {
    background: url(/Images/gg_pattern.png) no-repeat center center;
    padding: 50px 0 0;
    margin: 5px 0;
}
#footer ul {
	padding: 0;
	overflow: hidden;
	display: inline-block;
	margin: 20px 0 40px;
}
#footer ul li {
    background: url(/Images/icons.png) no-repeat center;
	float: left;
